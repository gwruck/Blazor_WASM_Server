{

  //***Update your Azure AD web app registration details or use Secret Manager
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "qualified.domain.name",
    "TenantId": "common",
    "ClientId": "[Client Id]",
    "ClientSecret": "[Client Secret]",
    "Scopes": "api://[Client Id]/access-as-user",
    "CallbackPath": "/signin-oidc"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
